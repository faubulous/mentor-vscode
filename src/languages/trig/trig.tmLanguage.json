{
    "scopeName": "source.trig",
    "uuid": "a8957d45-3e20-42d1-8293-3113293f145c",
    "patterns": [
        {
            "include": "#comment"
        },
        {
            "include": "#directive"
        },
        {
            "include": "#graph"
        },
        {
            "include": "#triple"
        }
    ],
    "repository": {
        "comment": {
            "name": "comment.line.number-sign.trig",
            "match": "#.*$"
        },
        "directive": {
            "begin": "(?i)^\\s*(@prefix|@base|BASE|PREFIX)",
            "beginCaptures": {
                "1": {
                    "name": "keyword.control.directive.trig"
                }
            },
            "end": "\\.",
            "endCaptures": {
                "0": {
                    "name": "punctuation.terminator.statement.trig"
                }
            },
            "patterns": [
                {
                    "include": "#comment"
                },
                {
                    "include": "#iri"
                },
                {
                    "name": "storage.type.prefix.trig",
                    "match": "([a-zA-Z0-9_]*)?:"
                }
            ]
        },
        "graph": {
            "begin": "(?i)\\b(GRAPH)\\s+",
            "beginCaptures": {
                "1": {
                    "name": "keyword.control.graph.trig"
                }
            },
            "end": "\\}",
            "endCaptures": {
                "0": {
                    "name": "punctuation.section.graph.end.trig"
                }
            },
            "patterns": [
                {
                    "include": "#comment"
                },
                {
                    "include": "#iri"
                },
                {
                    "include": "#prefixed_name"
                },
                {
                    "name": "punctuation.section.graph.begin.trig",
                    "match": "\\{"
                },
                {
                    "include": "#triple"
                }
            ]
        },
        "triple": {
            "patterns": [
                {
                    "include": "#comment"
                },
                {
                    "include": "#iri"
                },
                {
                    "include": "#prefixed_name"
                },
                {
                    "include": "#literal"
                },
                {
                    "include": "#blank_node"
                },
                {
                    "include": "#collection"
                },
                {
                    "name": "keyword.control.trig",
                    "match": "\\b(a)\\b"
                },
                {
                    "name": "constant.language.boolean.trig",
                    "match": "\\b(true|false)\\b"
                },
                {
                    "name": "punctuation.terminator.statement.trig",
                    "match": "\\."
                },
                {
                    "name": "punctuation.separator.predicate-object-list.trig",
                    "match": ";"
                },
                {
                    "name": "punctuation.separator.object-list.trig",
                    "match": ","
                }
            ]
        },
        "iri": {
            "name": "variable.other.constant.trig",
            "match": "<[^>]*>"
        },
        "prefixed_name": {
            "match": "(([a-zA-Z0-9_]*)?(:))([a-zA-Z0-9_\\.-]*)?",
            "captures": {
                "1": {
                    "name": "storage.type.prefix.trig"
                }
            }
        },
        "literal": {
            "patterns": [
                {
                    "name": "string.quoted.triple.trig",
                    "begin": "(\"\"\")|(''')",
                    "end": "(\\1)",
                    "patterns": [
                        {
                            "name": "constant.character.escape.trig",
                            "match": "\\\\."
                        }
                    ]
                },
                {
                    "name": "string.quoted.double.trig",
                    "begin": "\"",
                    "end": "\"",
                    "patterns": [
                        {
                            "name": "constant.character.escape.trig",
                            "match": "\\\\."
                        }
                    ]
                },
                {
                    "name": "string.quoted.single.trig",
                    "begin": "'",
                    "end": "'",
                    "patterns": [
                        {
                            "name": "constant.character.escape.trig",
                            "match": "\\\\."
                        }
                    ]
                },
                {
                    "name": "constant.numeric.trig",
                    "match": "[+-]?[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?"
                },
                {
                    "name": "entity.name.function.trig",
                    "match": "@[a-zA-Z-]+"
                },
                {
                    "begin": "(\\^\\^)(\\s*)(<[^>]*>|(([a-zA-Z0-9_]*)?(:))([a-zA-Z0-9_\\.-]*)?)",
                    "beginCaptures": {
                        "1": { "name": "entity.name.function.trig" },
                        "3": { "name": "entity.name.function.trig" }
                    },
                    "end": "(?=[\\s.,;\\]\\)])",
                    "name": "entity.name.function.trig"
                }
            ]
        },
        "blank_node": {
            "patterns": [
                {
                    "name": "variable.other.blank-node.labeled.trig",
                    "match": "_:[a-zA-Z0-9_]+"
                },
                {
                    "name": "variable.other.blank-node.anonymous.trig",
                    "match": "\\[\\s*\\]"
                },
                {
                    "begin": "\\[",
                    "end": "\\]",
                    "name": "variable.other.blank-node.property-list.trig",
                    "patterns": [
                        {
                            "include": "#triple"
                        }
                    ]
                }
            ]
        },
        "collection": {
            "begin": "\\(",
            "end": "\\)",
            "name": "meta.collection.trig",
            "patterns": [
                {
                    "include": "#comment"
                },
                {
                    "include": "#iri"
                },
                {
                    "include": "#prefixed_name"
                },
                {
                    "include": "#literal"
                },
                {
                    "include": "#blank_node"
                }
            ]
        }
    }
}