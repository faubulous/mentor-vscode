{
	"comments": {
		"lineComment": "#"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	  ],
	"autoClosingPairs": [
		{
			"open": "<",
			"close": ">",
			"notIn": [
				"string",
				"comment"
			]
		}
	],
	"autoCloseBefore": ";.\n\t",
	"folding": {
		"markers": {
			"start": "^\\s*(<.+>|.*:.*)",
			"end": "\\.$"
		}
	}
}