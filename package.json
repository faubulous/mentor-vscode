{
  "name": "mentor",
  "displayName": "Mentor",
  "description": "Code editing support for RDF, RDFS, OWL, SKOS, SHACL and SPARQL.",
  "keywords": [
    "anzo",
    "blazegraph",
    "graphdb",
    "jena",
    "json-ld",
    "knowledge graph",
    "linked data",
    "n3",
    "neptune",
    "nquads",
    "ntriples",
    "ontology",
    "owl",
    "protege",
    "rdf",
    "rdf4j",
    "rdfa",
    "rdfox",
    "rdfs",
    "semantic web",
    "shacl",
    "skos",
    "sparql",
    "stardog",
    "taxonomy",
    "thesaurus",
    "terminology",
    "trig",
    "turtle",
    "vocabulary"
  ],
  "icon": "media/icons/mentor.png",
  "version": "0.3.0",
  "publisher": "faubulous",
  "private": false,
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/faubulous/mentor-vscode"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "browser": "./out/extension.js",
  "scripts": {
    "clean": "rm -Rf node_modules && rm -Rf out && rm -Rf media/*.woff",
    "build": "npm run build:font && node ./build.js",
    "build:watch": "npm run build:font && node ./build.js --watch",
    "build:font": "node ./build-font.mjs",
    "compile": "npm run build:font && tsc -p tsconfig.json",
    "package": "npm run build:font && node ./build.js --production && vsce package --dependencies -o out/mentor.vsix",
    "package:install": "npm run package && code --install-extension out/mentor.vsix --force",
    "test:web": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@rdfjs/types": "^2.0.1",
    "@types/n3": "^1.26.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/vscode": "^1.90.0",
    "@types/vscode-notebook-renderer": "^1.72.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vscode-elements/react-elements": "^1.15.1",
    "@vscode/test-web": "^0.0.70",
    "esbuild": "^0.25.5",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^9.29.0",
    "glob": "^11.0.3",
    "svgtofont": "^6.3.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vscode-uri": "^3.1.0"
  },
  "dependencies": {
    "@comunica/query-sparql-rdfjs-lite": "^4.3.0",
    "@faubulous/mentor-rdf": "^0.2.55",
    "@vscode/codicons": "^0.0.36",
    "millan": "^5.4.0",
    "n3": "^1.26.0",
    "react": "^19.1.0",
    "sax-ts": "^1.2.13",
    "setimmediate": "^1.0.5",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  },
  "activationEvents": [
    "onLanguage:xml",
    "onNotebook:mentor-notebook"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mentor",
          "title": "Mentor",
          "icon": "media/glyphs/ue000-mentor.svg"
        }
      ],
      "panel": [
        {
          "id": "mentor-sparql-results",
          "title": "SPARQL",
          "icon": "media/glyphs/ue000-mentor.svg"
        }
      ]
    },
    "views": {
      "mentor": [
        {
          "id": "mentor.view.workspaceTree",
          "name": "Workspace",
          "icon": "media/glyphs/ue000-mentor.svg"
        },
        {
          "id": "mentor.view.definitionTree",
          "name": "Definitions",
          "icon": "media/glyphs/ue000-mentor.svg"
        }
      ],
      "mentor-sparql-results": [
        {
          "type": "webview",
          "id": "mentor.sparqlResultsView",
          "name": "Results",
          "icon": "media/glyphs/ue000-mentor.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "mentor.view.workspaceTree",
        "contents": "Welcome to the Mentor extension for Visual Studio Code.\n\n This view shows all RDF files in your current workspace for quick access. To get started, create a file with one of the following extensions:\n\n[Open Explorer](command:workbench.view.explorer)\n\n*.ttl, *.trig, *.n3, *.nt, *.nq, *.rdf, *.sparql, *.rq",
        "when": "!mentor.workspace.isInitializing && mentor.workspace.isEmpty"
      }
    ],
    "keybindings": [
      {
        "command": "mentor.command.executeSparqlQueryFromActiveEditor",
        "key": "meta+enter",
        "when": "editorLangId == 'sparql' && !notebookCellType"
      }
    ],
    "notebooks": [
      {
        "type": "mentor-notebook",
        "displayName": "Mentor Notebook",
        "selector": [
          {
            "filenamePattern": "*.mnb"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "mentor.notebook.sparqlResultsRenderer",
        "displayName": "SPARQL Results Renderer",
        "entrypoint": "./out/sparql-results-notebook-renderer.js",
        "notebookType": "mentor-notebook",
        "requiresMessaging": "always",
        "mimeTypes": [
          "application/sparql-results+json"
        ]
      }
    ],
    "languages": [
      {
        "id": "ntriples",
        "aliases": [
          "N-Triples",
          "ntriples"
        ],
        "extensions": [
          ".nt",
          ".n3"
        ],
        "configuration": "./out/turtle-language-config.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "nquads",
        "aliases": [
          "N-Quads",
          "nquads"
        ],
        "extensions": [
          ".nq"
        ],
        "configuration": "./out/turtle-language-config.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "turtle",
        "aliases": [
          "Turtle",
          "ttl"
        ],
        "extensions": [
          ".ttl"
        ],
        "configuration": "./out/turtle-language-config.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "trig",
        "aliases": [
          "TriG",
          "trig"
        ],
        "extensions": [
          ".trig"
        ],
        "configuration": "./out/trig-language-config.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "sparql",
        "aliases": [
          "SPARQL",
          "sparql"
        ],
        "extensions": [
          ".sparql",
          ".rq"
        ],
        "configuration": "./out/sparql-language-config.json",
        "icon": {
          "light": "./media/icons/sparql-light.svg",
          "dark": "./media/icons/sparql-dark.svg"
        }
      },
      {
        "id": "xml",
        "aliases": [
          "RDF/XML",
          "rdf/xml"
        ],
        "extensions": [
          ".rdf"
        ],
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      }
    ],
    "colors": [
      {
        "id": "mentor.color.concept",
        "description": "Color used for tree nodes representing concepts or concept references.",
        "defaults": {
          "light": "#901485",
          "dark": "#569cd6"
        }
      },
      {
        "id": "mentor.color.class",
        "description": "Color used for tree nodes representing class or class references.",
        "defaults": {
          "light": "#007acc",
          "dark": "#569cd6"
        }
      },
      {
        "id": "mentor.color.individual",
        "description": "Color used for tree nodes representing literals.",
        "defaults": {
          "light": "#007acc",
          "dark": "#569cd6"
        }
      },
      {
        "id": "mentor.color.objectProperty",
        "description": "Color used for tree nodes representing object properties.",
        "defaults": {
          "light": "#006bd5",
          "dark": "#569cd6"
        }
      },
      {
        "id": "mentor.color.dataProperty",
        "description": "Color used for tree nodes representing data property.",
        "defaults": {
          "light": "#a31515",
          "dark": "#ce9178"
        }
      },
      {
        "id": "mentor.color.token.keyword",
        "description": "Color used for keywords in the editor.",
        "defaults": {
          "light": "#C586C0",
          "dark": "#C586C0"
        }
      },
      {
        "id": "mentor.color.token.comment",
        "description": "Color used for comments in the editor.",
        "defaults": {
          "light": "#6A9955",
          "dark": "#6A9955"
        }
      },
      {
        "id": "mentor.color.token.localName",
        "description": "Color used for local names in the editor.",
        "defaults": {
          "light": "#CCCCCC",
          "dark": "#CCCCCC"
        }
      },
      {
        "id": "mentor.color.token.iri",
        "description": "Color used for IRIs in the editor.",
        "defaults": {
          "light": "#4FC1FF",
          "dark": "#4FC1FF"
        }
      },
      {
        "id": "mentor.color.token.prefix",
        "description": "Color used for namespace prefixes in the editor.",
        "defaults": {
          "light": "#569CD6",
          "dark": "#569CD6"
        }
      },
      {
        "id": "mentor.color.token.booleanLiteral",
        "description": "Color used for boolean literals in the editor.",
        "defaults": {
          "light": "#569CD6",
          "dark": "#569CD6"
        }
      },
      {
        "id": "mentor.color.token.numericLiteral",
        "description": "Color used for numeric literals in the editor.",
        "defaults": {
          "light": "#B5CEA8",
          "dark": "#B5CEA8"
        }
      },
      {
        "id": "mentor.color.token.quotedLiteral",
        "description": "Color used for quoted literals in the editor.",
        "defaults": {
          "light": "#CE9178",
          "dark": "#CE9178"
        }
      },
      {
        "id": "mentor.color.token.datatypeIri",
        "description": "Color used for datatype IRIs in the editor.",
        "defaults": {
          "light": "#DCDCAA",
          "dark": "#DCDCAA"
        }
      },
      {
        "id": "mentor.color.token.languageTag",
        "description": "Color used for language tags in the editor.",
        "defaults": {
          "light": "#DCDCAA",
          "dark": "#DCDCAA"
        }
      }
    ],
    "configurationDefaults": {
      "[ntriples]": {
        "editor.wordBasedSuggestionsMode": "allDocuments",
        "editor.semanticHighlighting.enabled": true
      },
      "[nquads]": {
        "editor.wordBasedSuggestionsMode": "allDocuments",
        "editor.semanticHighlighting.enabled": true
      },
      "[turtle]": {
        "editor.wordBasedSuggestionsMode": "allDocuments",
        "editor.semanticHighlighting.enabled": true
      },
      "[trig]": {
        "editor.wordBasedSuggestionsMode": "allDocuments",
        "editor.semanticHighlighting.enabled": true
      },
      "[sparql]": {
        "editor.wordBasedSuggestionsMode": "allDocuments",
        "editor.semanticHighlighting.enabled": true
      },
      "[xml]": {
        "editor.wordBasedSuggestionsMode": "allDocuments",
        "editor.semanticHighlighting.enabled": true
      }
    },
    "commands": [
      {
        "command": "mentor.command.createNotebook",
        "title": "Mentor: Create Notebook",
        "icon": "$(notebook)"
      },
      {
        "command": "mentor.command.createNotebookFromEditor",
        "title": "Mentor: Create Notebook from Editor",
        "icon": "$(notebook)"
      },
      {
        "command": "mentor.command.createSparqlQueryFile",
        "title": "Mentor: Create SPARQL Query File",
        "icon": "$(new-file)"
      },
      {
        "command": "mentor.command.executeSparqlQueryFromActiveEditor",
        "title": "Mentor: Execute SPARQL Query",
        "icon": "$(play)"
      },
      {
        "command": "mentor.command.saveSparqlQueryResults",
        "title": "Mentor: Save SPARQL Query Results",
        "icon": "$(save-as)"
      },
      {
        "command": "mentor.command.analyzeWorkspace",
        "title": "Find Problems",
        "icon": "$(debug-alt)"
      },
      {
        "command": "mentor.command.updatePrefixes",
        "title": "Mentor: Update prefix definitions from prefix.cc",
        "icon": "$(cloud-download)"
      },
      {
        "command": "mentor.command.openSettings",
        "title": "Settings",
        "icon": "$(gear)"
      },
      {
        "command": "mentor.command.findReferences",
        "title": "Find All References",
        "group": "navigation"
      },
      {
        "command": "mentor.command.revealDefinition",
        "title": "Go to Definition",
        "icon": "$(go-to-file)"
      },
      {
        "command": "mentor.command.revealShapeDefinition",
        "title": "Go to Shape Definition",
        "icon": "$(go-to-shape)"
      },
      {
        "command": "mentor.command.openInBrowser",
        "title": "Open in Browser",
        "icon": "$(globe)"
      },
      {
        "command": "mentor.command.showAnnotatedLabels",
        "title": "Show Annotated Labels",
        "icon": "$(rdf-label-tag)"
      },
      {
        "command": "mentor.command.showUriLabels",
        "title": "Show URI Labels",
        "icon": "$(rdf-label-uri)"
      },
      {
        "command": "mentor.command.showUriLabelsWithPrefix",
        "title": "Show URI Labels with Prefix",
        "icon": "$(rdf-label-uri)"
      },
      {
        "command": "mentor.command.groupDefinitionsByType",
        "title": "Group by Type"
      },
      {
        "command": "mentor.command.groupDefinitionsBySource",
        "title": "Group by Source of Definition"
      },
      {
        "command": "mentor.command.refreshDefinitionsTree",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mentor.command.showReferences",
        "title": "Show Referenced Resources",
        "icon": "$(rdf-class)"
      },
      {
        "command": "mentor.command.hideReferences",
        "title": "Hide Referenced Resources",
        "icon": "$(rdf-class-ref)"
      },
      {
        "command": "mentor.command.showPropertyTypes",
        "title": "Show Property Types",
        "icon": "$(rdf-tree)"
      },
      {
        "command": "mentor.command.hidePropertyTypes",
        "title": "Hide Property Types",
        "icon": "$(rdf-list)"
      },
      {
        "command": "mentor.command.showIndividualTypes",
        "title": "Show Individual Types",
        "icon": "$(rdf-tree)"
      },
      {
        "command": "mentor.command.hideIndividualTypes",
        "title": "Hide Individual Types",
        "icon": "$(rdf-list)"
      },
      {
        "command": "mentor.command.deletePrefixes",
        "title": "Delete prefix definitions"
      },
      {
        "command": "mentor.command.implementPrefixes",
        "title": "Implement prefix definitions"
      },
      {
        "command": "mentor.command.sortPrefixes",
        "title": "Sort prefix definitions"
      },
      {
        "command": "mentor.command.selectActiveLanguage",
        "title": "Select Active Language",
        "icon": "$(select-language)"
      },
      {
        "command": "mentor.command.highlightNamespaceDefinitions",
        "title": "Mentor: Highlight Namespace Definitions"
      },
      {
        "command": "mentor.command.highlightTypeDefinitions",
        "title": "Mentor: Highlight Type Definitions"
      },
      {
        "command": "mentor.command.highlightTypeAssertions",
        "title": "Mentor: Highlight Type Assertions"
      },
      {
        "command": "mentor.command.highlightReferencedIris",
        "title": "Mentor: Highlight Referenced IRIs"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "mentor.command.executeSparqlQueryFromActiveEditor",
          "when": "editorLangId == 'sparql'",
          "group": "navigation@0"
        },
        {
          "command": "mentor.command.createNotebookFromEditor",
          "when": "editorLangId == 'sparql'",
          "group": "navigation@1"
        }
      ],
      "view/title": [
        {
          "command": "mentor.command.analyzeWorkspace",
          "when": "view == mentor.view.workspaceTree && !mentor.isInitializing && !mentor.workspace.isIndexing",
          "group": "navigation@3"
        },
        {
          "command": "mentor.command.groupDefinitionsByType",
          "when": "view == mentor.view.definitionTree",
          "group": "2_workspace "
        },
        {
          "command": "mentor.command.groupDefinitionsBySource",
          "when": "view == mentor.view.definitionTree",
          "group": "2_workspace "
        },
        {
          "command": "mentor.command.showAnnotatedLabels",
          "when": "view == mentor.view.definitionTree",
          "group": "4_search"
        },
        {
          "command": "mentor.command.showUriLabels",
          "when": "view == mentor.view.definitionTree",
          "group": "4_search"
        },
        {
          "command": "mentor.command.showUriLabelsWithPrefix",
          "when": "view == mentor.view.definitionTree",
          "group": "4_search"
        },
        {
          "command": "mentor.command.openSettings",
          "group": "z_commands",
          "when": "view == mentor.view.workspaceTree || view == mentor.view.definitionTree"
        },
        {
          "command": "mentor.command.showReferences",
          "when": "view == mentor.view.definitionTree && view.showReferences === false",
          "group": "navigation@2"
        },
        {
          "command": "mentor.command.hideReferences",
          "when": "view == mentor.view.definitionTree && view.showReferences === true",
          "group": "navigation@2"
        },
        {
          "command": "mentor.command.selectActiveLanguage",
          "when": "view == mentor.view.definitionTree",
          "group": "navigation@1"
        },
        {
          "command": "mentor.command.refreshDefinitionsTree",
          "when": "view == mentor.view.definitionTree",
          "group": "navigation@99"
        }
      ],
      "view/item/context": [
        {
          "command": "mentor.command.findReferences",
          "when": "viewItem =~ /resource/",
          "group": "4_search"
        },
        {
          "command": "mentor.command.openInBrowser",
          "when": "viewItem =~ /resource/",
          "group": "z_commands"
        },
        {
          "command": "mentor.command.revealShapeDefinition",
          "when": "viewItem != shapes && viewItem =~ / shape-target/",
          "group": "inline"
        },
        {
          "command": "mentor.command.showPropertyTypes",
          "when": "view.showPropertyTypes === false && viewItem == properties",
          "group": "inline"
        },
        {
          "command": "mentor.command.hidePropertyTypes",
          "when": "view.showPropertyTypes === true && viewItem == properties",
          "group": "inline"
        },
        {
          "command": "mentor.command.showIndividualTypes",
          "when": "view.showIndividualTypes === false && viewItem == individuals",
          "group": "inline"
        },
        {
          "command": "mentor.command.hideIndividualTypes",
          "when": "view.showIndividualTypes === true && viewItem == individuals",
          "group": "inline"
        }
      ]
    },
