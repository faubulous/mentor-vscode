{
  "name": "mentor",
  "displayName": "Custom view Samples",
  "description": "Samples for VSCode's view API",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build-font": "node build/update-font.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "npx jest"
  },
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:jsonc"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "icons": {
      "app-mentor": {
        "description": "Mentor icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e000"
        }
      },
      "rdf-class": {
        "description": "Class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e001"
        }
      },
      "rdf-class-eq": {
        "description": "Equivalent class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e002"
        }
      },
      "rdf-class-ref": {
        "description": "Referenced class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e003"
        }
      },
      "rdf-class-ref-eq": {
        "description": "Referenced equivalent class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e004"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "class-explorer",
          "title": "Mentor",
          "icon": "media/icons/0-mentor.svg"
        }
      ]
    },
    "views": {
      "class-explorer": [
        {
          "id": "classExplorer",
          "name": "Classes",
          "icon": "media/icons/0-mentor.svg",
          "contextualTitle": "Classes"
        }
      ],
      "explorer": [
        {
          "id": "jsonOutline",
          "name": "Json Outline",
          "when": "jsonOutlineEnabled"
        },
        {
          "id": "testViewDragAndDrop",
          "name": "Test View Drag and Drop"
        }
      ]
    },
    "commands": [
      {
        "command": "classExplorer.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "classExplorer.addEntry",
        "title": "Add"
      },
      {
        "command": "classExplorer.editEntry",
        "title": "Edit",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "classExplorer.deleteEntry",
        "title": "Delete"
      },
      {
        "command": "jsonOutline.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "jsonOutline.refreshNode",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "jsonOutline.renameNode",
        "title": "Rename"
      },
      {
        "command": "testView.reveal",
        "title": "Test View: Reveal"
      },
      {
        "command": "testView.changeTitle",
        "title": "Test View: Change Title"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "classExplorer.refreshEntry",
          "when": "view == nodeDependencies",
          "group": "navigation"
        },
        {
          "command": "jsonOutline.renameNode",
          "when": "view == jsonOutline"
        },
        {
          "command": "classExplorer.addEntry",
          "when": "view == nodeDependencies"
        },
        {
          "command": "jsonOutline.refresh",
          "when": "view == jsonOutline",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "classExplorer.editEntry",
          "when": "view == nodeDependencies && viewItem == dependency",
          "group": "inline"
        },
        {
          "command": "classExplorer.deleteEntry",
          "when": "view == nodeDependencies && viewItem == dependency"
        },
        {
          "command": "jsonOutline.renameNode",
          "when": "view == jsonOutline"
        },
        {
          "command": "jsonOutline.refreshNode",
          "when": "view == jsonOutline",
          "group": "inline"
        }
      ]
    },
    "colors": [
      {
        "id": "mentor.colors.www.w3.org.1999.02.22.rdf.syntax.ns",
        "description": "Color used for the W3C namespace URIs.",
        "defaults": {
          "light": "#0771DE",
          "dark": "#0771DE"
        }
      },
      {
        "id": "mentor.colors.www.w3.org.2000.01.rdf.schema",
        "description": "Color used for the W3C namespace URIs.",
        "defaults": {
          "light": "#0771DE",
          "dark": "#0771DE"
        }
      },
      {
        "id": "mentor.colors.www.w3.org.2001.XMLSchema",
        "description": "Color used for the W3C namespace URIs.",
        "defaults": {
          "light": "#0771DE",
          "dark": "#0771DE"
        }
      },
      {
        "id": "mentor.colors.www.w3.org.2002.07.owl",
        "description": "Color used for the W3C namespace URIs.",
        "defaults": {
          "light": "#0771DE",
          "dark": "#0771DE"
        }
      },
      {
        "id": "mentor.colors.www.w3.org.ns.rdfa",
        "description": "Color used for the W3C namespace URIs.",
        "defaults": {
          "light": "#0771DE",
          "dark": "#0771DE"
        }
      },
      {
        "id": "mentor.colors.xmlns.com.foaf.0.1",
        "description": "Color used for the FOAF namespace URIs.",
        "defaults": {
          "light": "#F7D633",
          "dark": "#F7D633"
        }
      },
      {
        "id": "mentor.colors.purl.org.dc.elements.1.1",
        "description": "Color used for the Dublin Core namespace URIs.",
        "defaults": {
          "light": "#FF9501",
          "dark": "#FF9501"
        }
      },
      {
        "id": "mentor.colors.purl.org.dc.dcmitype",
        "description": "Color used for the Dublin Core namespace URIs.",
        "defaults": {
          "light": "#FF9501",
          "dark": "#FF9501"
        }
      },
      {
        "id": "mentor.colors.schema.org",
        "description": "Color used for the Schema.org namespace URIs.",
        "defaults": {
          "light": "#bc0d0d",
          "dark": "#bc0d0d"
        }
      },
      {
        "id": "mentor.colors.w3id.org.semanticarts.ns.ontology.gist",
        "description": "Color used for the Gist namespace URIs.",
        "defaults": {
          "light": "#005A7D",
          "dark": "#005A7D"
        }
      }
    ],
    "configuration": [
      {
        "type": "object",
        "id": "mentorSettings",
        "title": "Mentor",
        "order": 1,
        "properties": {
          "mentor.namespaces": {
            "type": "array",
            "title": "Namespaces",
            "description": "Manage the namespace URIs to be used in vocabularies.",
            "items": {
              "type": "object",
              "properties": {
                "uri": {
                  "type": "string",
                  "description": "The namespace URI."
                },
                "defaultPrefix": {
                  "type": "string",
                  "description": "The default abbreviation to be used for the namespace URI if no other is defined."
                }
              }
            },
            "default": [
              {
                "uri": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
                "defaultPrefix": "rdf"
              },
              {
                "uri": "http://www.w3.org/2000/01/rdf-schema#",
                "defaultPrefix": "rdfs"
              },
              {
                "uri": "http://www.w3.org/ns/rdfa#",
                "defaultPrefix": "rdfa"
              },
              {
                "uri": "http://www.w3.org/2002/07/owl#",
                "defaultPrefix": "owl"
              },
              {
                "uri": "http://www.w3.org/2004/02/skos/core#",
                "defaultPrefix": "skos"
              },
              {
                "uri": "http://schema.org/",
                "defaultPrefix": "schema"
              },
              {
                "uri": "https://schema.org/",
                "defaultPrefix": "schema"
              }
            ]
          }
        }
      }
    ]
  },
  "devDependencies": {
    "@types/ftp": "^0.3.33",
    "@types/jest": "^29.5.5",
    "@types/mkdirp": "^0.5.2",
    "@types/n3": "^1.16.0",
    "@types/node": "^16.18.34",
    "@types/rimraf": "^2.0.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "webfont": "^11.2.26"
  },
  "dependencies": {
    "@faubulous/mentor-rdf": "^0.1.2",
    "ftp": "^0.3.10",
    "jsonc-parser": "^0.4.2",
    "minimist": "^1.2.6",
    "mkdirp": "^0.5.1",
    "n3": "^1.17.1",
    "rimraf": "^2.6.2"
  }
}
