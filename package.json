{
  "name": "mentor",
  "displayName": "Mentor",
  "description": "Create RDF ontologies and knowledge graph projects with Visual Studio code.",
  "keywords": [
    "rdf",
    "rdfs",
    "owl",
    "shacl",
    "ontology",
    "vocabulary"
  ],
  "icon": "media/icons/mentor.png",
  "version": "0.1.0",
  "publisher": "faubulous",
  "private": false,
  "license": "GPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/faubulous/mentor-vscode"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "build": "node ./build-font.js && node ./build.js",
    "build:watch": "node ./build-font.js && node ./build.js --watch",
    "build:font": "node ./build-font.js",
    "package": "set NODE_ENV=production && npm run build && vsce package --dependencies -o out/mentor.vsix",
    "package:install": "npm run package && code --install-extension out/mentor.vsix --force",
    "test": "npx jest",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/mkdirp": "^0.5.2",
    "@types/n3": "^1.16.0",
    "@types/node": "^16.18.34",
    "@types/rimraf": "^2.0.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "esbuild": "^0.16.10",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^8.26.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "webfont": "^11.2.26"
  },
  "dependencies": {
    "@faubulous/mentor-rdf": "^0.2.1",
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "millan": "^5.3.0",
    "n3": "^1.17.1",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  },
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "ntriples",
        "aliases": [
          "N-Triples",
          "ntriples"
        ],
        "extensions": [
          ".nt",
          ".n3"
        ],
        "configuration": "./src/languages/language-config-turtle.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "nquads",
        "aliases": [
          "N-Quads",
          "nquads"
        ],
        "extensions": [
          ".nq"
        ],
        "configuration": "./src/languages/language-config-turtle.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "turtle",
        "aliases": [
          "Turtle",
          "ttl"
        ],
        "extensions": [
          ".ttl"
        ],
        "configuration": "./src/languages/language-config-turtle.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "trig",
        "aliases": [
          "TriG",
          "trig"
        ],
        "extensions": [
          ".trig"
        ],
        "configuration": "./src/languages/language-config-trig.json",
        "icon": {
          "light": "./media/icons/ontology-light.svg",
          "dark": "./media/icons/ontology-dark.svg"
        }
      },
      {
        "id": "sparql",
        "aliases": [
          "SPARQL",
          "sparql"
        ],
        "extensions": [
          ".sparql"
        ],
        "configuration": "./src/languages/language-config-sparql.json",
        "icon": {
          "light": "./media/icons/sparql-light.svg",
          "dark": "./media/icons/sparql-dark.svg"
        }
      }
    ],
    "semanticTokenScopes": [
      {
        "language": "turtle",
        "scopes": {
          "namespace": [
            "storage.modifier.turtle"
          ]
        }
      },
      {
        "language": "sparql",
        "scopes": {
          "namespace": [
            "storage.modifier.turtle"
          ]
        }
      }
    ],
    "icons": {
      "app-mentor": {
        "description": "Mentor icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e000"
        }
      },
      "rdf-class": {
        "description": "Class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e001"
        }
      },
      "rdf-class-eq": {
        "description": "Equivalent class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e002"
        }
      },
      "rdf-class-ref": {
        "description": "Referenced class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e003"
        }
      },
      "rdf-class-ref-on": {
        "description": "Referenced class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e004"
        }
      },
      "rdf-class-ref-off": {
        "description": "Referenced class icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e005"
        }
      },
      "rdf-individual": {
        "description": "Individual icon",
        "default": {
          "fontPath": "./media/mentor-icons.woff",
          "fontCharacter": "\\e006"
        }
      }
    },
    "colors": [
      {
        "id": "mentor.color.class",
        "description": "Color used for tree nodes representing class or class references.",
        "defaults": {
          "light": "#af00e2",
          "dark": "#c586c0"
        }
      },
      {
        "id": "mentor.color.individual",
        "description": "Color used for tree nodes representing literals.",
        "defaults": {
          "light": "#af00e2",
          "dark": "#c586c0"
        }
      },
      {
        "id": "mentor.color.objectProperty",
        "description": "Color used for tree nodes representing object properties.",
        "defaults": {
          "light": "#af00e2",
          "dark": "#c586c0"
        }
      },
      {
        "id": "mentor.color.dataProperty",
        "description": "Color used for tree nodes representing data property.",
        "defaults": {
          "light": "#a31515",
          "dark": "#ce9178"
        }
      },
      {
        "id": "mentor.color.annotationProperty",
        "description": "Color used for tree nodes representing annotation properties.",
        "defaults": {
          "light": "#005500",
          "dark": "#6a9742"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mentor",
          "title": "Mentor",
          "icon": "media/glyphs/0-mentor.svg"
        }
      ]
    },
    "views": {
      "mentor": [
        {
          "id": "mentor.view.classTree",
          "name": "Classes"
        },
        {
          "id": "mentor.view.propertyTree",
          "name": "Properties"
        },
        {
          "id": "mentor.view.individualTree",
          "name": "Individuals"
        }
      ]
    },
    "commands": [
      {
        "command": "mentor.action.openSettings",
        "title": "Settings",
        "icon": "$(gear)"
      },
      {
        "command": "mentor.action.refreshClassTree",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mentor.action.showUsages",
        "title": "Show Properties and Individuals",
        "icon": "$(find-selection)"
      },
      {
        "command": "mentor.action.showReferencedClasses",
        "title": "Show Referenced Classes",
        "icon": "$(rdf-class-ref)"
      },
      {
        "command": "mentor.action.hideReferencedClasses",
        "title": "Hide Referenced Classes",
        "icon": "$(rdf-class-ref-off)"
      },
      {
        "command": "mentor.action.refreshPropertyTree",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mentor.action.showPropertyTypes",
        "title": "Show Property Types",
        "icon": "$(rdf-class-ref)"
      },
      {
        "command": "mentor.action.hidePropertyTypes",
        "title": "Hide Property Types",
        "icon": "$(rdf-class-ref-off)"
      },
      {
        "command": "mentor.action.refreshIndividualTree",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mentor.action.showIndividualTypes",
        "title": "Show Individual Types",
        "icon": "$(rdf-class-ref)"
      },
      {
        "command": "mentor.action.hideIndividualTypes",
        "title": "Hide Individual Types",
        "icon": "$(rdf-class-ref-off)"
      },
      {
        "command": "mentor.action.findReferences",
        "title": "Find All References",
        "group": "navigation"
      },
      {
        "command": "mentor.action.revealDefinition",
        "title": "Go to Definition",
        "icon": "$(go-to-file)"
      },
      {
        "command": "mentor.action.openInBrowser",
        "title": "Open in Browser",
        "icon": "$(globe)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "mentor.action.showReferencedClasses",
          "when": "view == mentor.view.classTree && classTree.showReferenced == false",
          "group": "navigation@2"
        },
        {
          "command": "mentor.action.hideReferencedClasses",
          "when": "view == mentor.view.classTree && classTree.showReferenced == true",
          "group": "navigation@2"
        },
        {
          "command": "mentor.action.refreshClassTree",
          "when": "view == mentor.view.classTree",
          "group": "navigation@3"
        },
        {
          "command": "mentor.action.showPropertyTypes",
          "when": "view == mentor.view.propertyTree && propertyTree.showTypes == false",
          "group": "navigation@1"
        },
        {
          "command": "mentor.action.hidePropertyTypes",
          "when": "view == mentor.view.propertyTree && propertyTree.showTypes == true",
          "group": "navigation@1"
        },
        {
          "command": "mentor.action.refreshPropertyTree",
          "when": "view == mentor.view.propertyTree",
          "group": "navigation@3"
        },
        {
          "command": "mentor.action.showIndividualTypes",
          "when": "view == mentor.view.individualTree && individualTree.showTypes == false",
          "group": "navigation@1"
        },
        {
          "command": "mentor.action.hideIndividualTypes",
          "when": "view == mentor.view.individualTree && individualTree.showTypes == true",
          "group": "navigation@1"
        },
        {
          "command": "mentor.action.refreshIndividualTree",
          "when": "view == mentor.view.individualTree",
          "group": "navigation@3"
        },
        {
          "command": "mentor.action.openSettings",
          "group": "2_configuration",
          "when": "view == mentor.view.classTree || view == mentor.view.propertyTree || view == mentor.view.individualTree"
        }
      ],
      "view/item/context": [
        {
          "command": "mentor.action.revealDefinition",
          "when": "viewItem == class || viewItem == property || viewItem == individual",
          "group": "navigation@1"
        },
        {
          "command": "mentor.action.findReferences",
          "when": "view == mentor.view.classTree || view == mentor.view.propertyTree || view == mentor.view.individualTree",
          "group": "navigation@2"
        },
        {
          "command": "mentor.action.openInBrowser",
          "when": "view == mentor.view.classTree || view == mentor.view.propertyTree || view == mentor.view.individualTree",
          "group": "navigation@3"
        }
      ]
    },
    "configurationDefaults": {
      "[sparql]": {
        "editor.wordBasedSuggestionsMode": "allDocuments"
      },
      "[turtle]": {
        "editor.wordBasedSuggestionsMode": "allDocuments"
      },
      "[trig]": {
        "editor.wordBasedSuggestionsMode": "allDocuments"
      },
      "[ntriples]": {
        "editor.wordBasedSuggestionsMode": "allDocuments"
      },
      "[nquads]": {
        "editor.wordBasedSuggestionsMode": "allDocuments"
      }
    },
    "configuration": [
      {
        "type": "object",
        "id": "mentorSettings",
        "title": "Mentor",
        "order": 1,
        "properties": {
          "mentor.internalBrowserEnabled": {
            "type": "boolean",
            "title": "Open URLs in interal browser",
            "description": "If enabled, URLs will be opened in the internal browser instead of the default browser.",
            "default": true
          },
          "mentor.predicates.label": {
            "type": "array",
            "title": "Label Predicates",
            "description": "Manage the predicates to be used for labels in the tree view for classes, properties and individuals.",
            "default": [
              "http://www.w3.org/2000/01/rdf-schema#label",
              "http://www.w3.org/2004/02/skos/core#prefLabel",
              "http://purl.org/dc/terms/title",
              "http://schema.org/name",
              "https://schema.org/name"
            ]
          },
          "mentor.predicates.description": {
            "type": "array",
            "title": "Description Predicates",
            "description": "Manage the predicates to be used for descriptions in the tree view for classes, properties and individuals.",
            "default": [
              "http://www.w3.org/2000/01/rdf-schema#comment",
              "http://www.w3.org/2004/02/skos/core#definition",
              "http://purl.org/dc/terms/description",
              "http://purl.org/dc/terms/abstract",
              "http://schema.org/description"
            ]
          },
          "mentor.namespaces": {
            "type": "array",
            "title": "Namespaces",
            "description": "Manage the namespace URIs to be used in vocabularies.",
            "items": {
              "type": "object",
              "properties": {
                "uri": {
                  "type": "string",
                  "description": "The namespace URI."
                },
                "defaultPrefix": {
                  "type": "string",
                  "description": "The default abbreviation to be used for the namespace URI if no other is defined."
                }
              }
            },
            "default": [
              {
                "uri": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
                "defaultPrefix": "rdf"
              },
              {
                "uri": "http://www.w3.org/2000/01/rdf-schema#",
                "defaultPrefix": "rdfs"
              },
              {
                "uri": "http://www.w3.org/ns/rdfa#",
                "defaultPrefix": "rdfa"
              },
              {
                "uri": "http://www.w3.org/2002/07/owl#",
                "defaultPrefix": "owl"
              },
              {
                "uri": "http://www.w3.org/2004/02/skos/core#",
                "defaultPrefix": "skos"
              },
              {
                "uri": "http://schema.org/",
                "defaultPrefix": "schema"
              },
              {
                "uri": "https://schema.org/",
                "defaultPrefix": "schema"
              }
            ]
          }
        },
        "mentor.language.turtle.maxNumberOfProblems": {
          "scope": "resource",
          "type": "number",
          "default": 100,
          "description": "Controls the maximum number of problems produced by the server."
        }
      }
    ]
  }
}